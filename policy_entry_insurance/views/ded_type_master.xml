<odoo>
     <record id="view_ded_type_form" model="ir.ui.view">
            <field name="name">ded.type.form</field>
            <field name="model">ded.type</field>
            <field name="arch" type="xml">
                <form string="Ded Type Master">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
     </record>
    <record id="view_ded_type_tree" model="ir.ui.view">
            <field name="name">ded.type.tree</field>
            <field name="model">ded.type</field>
            <field name="arch" type="xml">
                <tree string="Ded Type Master">
                    <field name="name"/>
                </tree>
            </field>
    </record>

     <record id="view_ded_type_search" model="ir.ui.view">
            <field name="name">vehicle.brand.search</field>
            <field name="model">ded.type</field>
            <field name="arch" type="xml">
                <search string="Ded Type Master">
                    <field name="name"/>
                </search>
            </field>
    </record>

     <record id="act_ded_type_master" model="ir.actions.act_window">
            <field name="name">Ded Type </field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ded.type</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_ded_type_tree"/>

        </record>
</odoo>